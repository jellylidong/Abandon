{
  "manifest_version": 2,

  "name": "Abandon",
  "description": "Angular CLI Chrome app. ",
  "version": "1.0",

  "browser_action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab"
  ],

  "content_security_policy": "script-src 'self' 'unsafe-eval' https://apis.google.com; object-src 'self'"
}
